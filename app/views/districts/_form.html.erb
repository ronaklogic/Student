<%= form_for @district, remote: true do |form| %>
  <div>
    <%= form.label :district_name %>
    <%= form.text_field :district_name %><br>
  </div>

  <div>
    <%= form.label :state_id %>  
    <%= form.select :state_id, State.all.collect { |p| [p.state_name, p.id] } %><br>
  </div>

  <div>
    <%= form.label :taluka_id %>
    <%= form.select :taluka_id, Taluka.all.collect { |p| [p.name, p.id] } %><br>
  </div>
  <%= form.submit %>
<% end %>

